<% frame ||= nil %>
<% has_prev = has_prev.nil? ? false : has_prev %>
<% no_children = frame.id && frame.has_children? ? "" : "no_children" %>
<% several_children = frame.id && frame.children.count > 1 ? "several_children" : "" %>
<div class="col-md-12 row">
  <div class="col-md-6 col-md-offset-3 frame central <%=no_children%> <%=several_children%>">
    <%= render partial: "frames/frame_view", locals: { frame: frame, has_prev: has_prev } %>
  </div>
</div>
