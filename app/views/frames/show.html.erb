<div class="page-header">
  <%= link_to ecomic_frames_path(@ecomic), class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_ecomic_frame_path(@ecomic, @frame), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show frame</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @frame.name %></dd>
  <dt>ecomic_id:</dt>
  <dd><%= @frame.ecomic_id %> : <%= @frame.ecomic_name %></dd>
  <dt>parent_id:</dt>
  <dd><%= @frame.parent_id %> : <%= @frame.parent_name %></dd>
  
  <dt>picture:</dt>
  <dd><%= image_tag @frame.picture.url %></dd>


</dl>
