<div class="page-header">
  <%= link_to ecomics_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_ecomic_path(@ecomic), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show ecomic</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @ecomic.name %></dd>

  <dt>Frames:</dt>
  <dd><%= link_to "Frames", ecomic_frames_path(@ecomic) %></dd>

</dl>
